	<queryString language="SQL">
		<![CDATA[SELECT
     transaksi.`idTrans` AS transaksi_idTrans,
     transaksi.`tglCheckin` AS transaksi_tglCheckin,
     transaksi.`tglCheckout` AS transaksi_tglCheckout,
     transaksi.`bayar` AS transaksi_bayar,
     tamu.`nama` AS tamu_nama,
     kategorikamar.`nama` AS kategorikamar_nama,
     kamar.`nama` AS kamar_nama,
     kamar.`harga` AS kamar_harga,
     kamar.`status` AS kamar_status
FROM
     `tamu` tamu INNER JOIN `transaksi` transaksi ON tamu.`idTamu` = transaksi.`idTamu`
     INNER JOIN `kamar` kamar ON transaksi.`noKamar` = kamar.`noKamar`
     INNER JOIN `kategorikamar` kategorikamar ON kamar.`idKategori` = kategorikamar.`idKategori`]]>
	</queryString>
